# pcrdb Configuration Example
# Copy this file to .env and fill in values

# Database
PCRDB_HOST=localhost
PCRDB_PORT=5432
PCRDB_DATABASE=your_database
PCRDB_USER=your_user
PCRDB_PASSWORD=your_password

# Task Queue
PCRDB_SYNC_NUM=10
PCRDB_BATCH_SIZE=30

# Shared Access Key (for all accounts)
PCRDB_ACCESS_KEY=your_access_key_here

# Game Config
TALENT_QUEST_TOTAL=250
